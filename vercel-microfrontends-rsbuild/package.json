{
  "name": "vercel-microfrontends-rsbuild",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:host\" \"npm run dev:header\" \"npm run dev:footer\"",
    "dev:host": "cd apps/host && npm run dev",
    "dev:header": "cd apps/remote-header && npm run dev",
    "dev:footer": "cd apps/remote-footer && npm run dev",
    "build": "npm run build:header && npm run build:footer && npm run build:host",
    "build:host": "cd apps/host && npm run build",
    "build:header": "cd apps/remote-header && npm run build",
    "build:footer": "cd apps/remote-footer && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@rsbuild/core": "^1.4.15",
    "@rsbuild/plugin-react": "^1.3.5",
    "@vercel/microfrontends": "^2.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@module-federation/enhanced": "^0.18.3",
    "@module-federation/rspack": "^0.18.3",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.7",
    "concurrently": "^9.2.0",
    "typescript": "^5.9.2"
  }
}
